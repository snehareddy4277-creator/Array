#include <stdio.h>

int main()
{
    int n,key,flag;
   printf("Enter the size of the array");
    scanf("%d",&n);
    printf("Enter the key");
    scanf("%d",&key);
    int arr1[n];
    for(int i=0;i<=n-1;i++)
{
    scanf("%d",&arr1[i]);
    if(arr1[1]==key)
    {
       flag=1;
       break;
   }
}
if(flag==0)
printf("key is not found");
else
printf("key is not found");
}
    
